[ignore]
.*/node_modules/.*
.*/dist/.*
.*/__tests__/.*
.*webpack\.config\.js
.*postcss\.config\.js
.*server\.js

[include]
src/

[libs]
flow_typed/

[options]
all=false
module.file_ext=.js
module.file_ext=.jsx
module.ignore_non_literal_requires=true
module.system.node.resolve_dirname=node_modules
module.use_strict=true
esproposal.decorators=ignore
include_warnings=true
